  },
  "dependencies": {
    "@babel/parser": "^7.0.0",
    "boxen": "^8.0.1",
    "chalk": "^5.4.1",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.0",
    "execa": "^9.6.0",
    "express": "^4.18.0",
    "fast-glob": "^3.3.3",
    "ora": "^8.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "scheduler": "^0.26.0",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/commander": "^2.12.0",
    "@types/node": "^24.0.10",
    "@types/react": "^18.2.62",
    "@types/react-dom": "^18.2.17",
    "@vitest/ui": "^3.2.4",
    "ts-morph": "^22.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "bin": {
    "storybookify": "cli/index.js"
  }
