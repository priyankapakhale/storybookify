{
  "name": "storybookify",
  "version": "0.1.0",
  "main": "cli/index.js",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/priyankapakhale/storybookify"
  },
  "author": "Priyanka Pakhale priyanka.pakhale54@gmail.com",
  "keywords": [
    "storybook",
    "react",
    "cli",
    "generator",
    "stories",
    "zero-config"
  ],
  "license": "MIT",
  "scripts": {
    "start": "npm run storybookify && npm run storybook",
    "build": "tsc",
    "generate": "node cli/index.js",
    "storybookify": "node cli/index.js",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint core cli utils --ext .js,.ts,.tsx",
    "dev:pack": "rm -rf node_modules && rm -rf dist && rm -rf build && npm install && npm run build && npm pack"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@babel/parser": "^7.0.0",
    "boxen": "^8.0.1",
    "chalk": "^5.4.1",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.0",
    "execa": "^9.6.0",
    "express": "^4.18.0",
    "fast-glob": "^3.3.3",
    "ora": "^8.2.0",
    "tsconfig-paths": "^4.2.0",
    "ts-morph": "^22.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/commander": "^2.12.0",
    "@types/node": "^24.0.10",
    "@types/react": "^18.2.62",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "bin": {
    "storybookify": "cli/index.js"
  },
  "files": [
    "dist/",
    "cli/"
  ],
  "preferGlobal": true
}
